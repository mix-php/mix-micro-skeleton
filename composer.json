{
  "name": "mix/mix-micro-skeleton",
  "description": "Microservice development skeleton",
  "type": "project",
  "keywords": [
    "mix",
    "skeleton",
    "microservice"
  ],
  "homepage": "https://openmix.org/mix-php",
  "license": "Apache-2.0",
