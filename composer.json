    {
      "name": "liu,jian",
      "email": "coder.keda@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "mix/console": "~2.2.0",
    "mix/bean": "~2.2.0",
    "mix/event": "~2.2.0",
    "mix/runtime": "~2.2.0",
    "mix/helper": "~2.2.0",
    "mix/monolog": "~2.2.0",
    "mix/validate": "~2.2.0",
    "mix/database": "~2.2.0",
    "mix/redis": "~2.2.0",
    "mix/redis-subscribe": "~2.2.0",
    "mix/cache": "~2.2.0",
    "mix/guzzle": "~2.2.0",
    "mix/auth": "~2.2.0",
    "mix/session": "~2.2.0",
    "mix/fast-route": "~2.2.0",
    "mix/view": "~2.2.0",
    "mix/websocket": "~2.2.0",
    "mix/sync-invoke": "~2.2.0",
    "mix/micro": "~2.2.0",
    "mix/micro-etcd": "~2.2.0",
    "mix/micro-hystrix": "~2.2.0",
    "mix/tracing": "~2.2.0",
    "mix/tracing-zipkin": "~2.2.0",
    "mix/worker-pool": "~2.2.0"
  },
  "require-dev": {
    "swoole/ide-helper": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "GPBMetadata\\": "protos/GPBMetadata/",
      "Php\\": "protos/Php/"
    },
    "files": [
      "app/functions.php"
    ]
  }
